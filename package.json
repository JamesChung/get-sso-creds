{
  "name": "get-sso-creds",
  "version": "6.3.0",
  "description": "CLI tool to retrieve or set AWS SSO credentials.",
  "author": "James Chung @JamesChung",
  "bin": {
    "gscreds": "./bin/run"
  },
  "homepage": "https://github.com/JamesChung/get-sso-creds",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "JamesChung/get-sso-creds",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.20.4",
    "@oclif/plugin-help": "^5.1.18",
    "@oclif/plugin-plugins": "^2.1.7",
    "aws-sdk": "^2.1253.0",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "clipboardy": "^2.3.0",
    "ini": "^3.0.1",
    "inquirer": "^8.2.5",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/ini": "^1.3.31",
    "@types/inquirer": "^8.2.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.9",
    "chai": "^4",
    "eslint-config-prettier": "^8.5.0",
    "globby": "^11",
    "mocha": "^10",
    "npm-check-updates": "^16.3.25",
    "oclif": "^3",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "gscreds",
    "dirname": "gscreds",
    "commands": "./dist/commands",
    "topicSeparator": " "
  },
  "scripts": {
    "build": "npm install && shx rm -rf dist && tsc -b && npm run build-all-bins",
    "build-all-bins": "npm run build-linux-bins && npm run build-macos-bins && npm run build-win-bins",
    "build-linux-bins": "pkg -o build/linux/get-sso-creds -t node16-linux-x64,node16-linux-arm64 .",
    "build-macos-bins": "pkg -o build/mac/get-sso-creds -t node16-macos-x64,node16-macos-arm64 .",
    "build-win-bins": "pkg -o build/win/get-sso-creds -t node16-win-x64,node16-win-arm64 .",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "deps-update": "ncu -u --target minor",
    "deploy": "npm run deps-update && npm install && npm run build && npm run version && npm publish"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/JamesChung/get-sso-creds/issues",
  "keywords": [
    "oclif",
    "aws",
    "cli",
    "sso"
  ],
  "types": "dist/index.d.ts"
}
