{
  "name": "get-sso-creds",
  "description": "CLI tool to get AWS SSO temporary credentials.",
  "version": "4.2.2",
  "author": "James Chung @JamesChung",
  "bin": {
    "gsc": "./bin/run"
  },
  "bugs": "https://github.com/JamesChung/get-sso-creds/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "aws-sdk": "^2.891.0",
    "chalk": "^4.1.1",
    "cli-ux": "^5.5.1",
    "ini": "^2.0.0",
    "inquirer": "^8.0.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.16",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.58",
    "chai": "^4.3.4",
    "globby": "^10.0.2",
    "mocha": "^8.3.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/JamesChung/get-sso-creds",
  "keywords": [
    "cli",
    "aws",
    "sso"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gsc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "JamesChung/get-sso-creds",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
